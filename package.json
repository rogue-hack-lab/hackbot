  "engines": {
    "node": "0.10.x"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "coffee-script": "^1.12.6",
    "hubot-test-helper": "1.9.0",
    "mocha": "5.2.0",
    "nodemon": "^1.18.9"
  },
  "scripts": {
    "test": "mocha --compilers \"coffee:coffee-script/register\" test/*",
    "test:watch": "mocha --watch --compilers \"coffee:coffee-script/register\" test/*",
